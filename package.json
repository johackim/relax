{
  "name": "relax",
  "version": "0.1.0",
  "description": "Gestion de file d'attente client pour auto-entrepreneur.",
  "scripts": {
    "setup": "cd admin && npm run setup",
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "NODE_ENV=production node server.js",
    "start:dev": "npm-run-all --parallel start:server watch:client",
    "start:server": "node server.js",
    "watch:client": "node watch.js",
    "build": "react-scripts build",
    "strapi": "node_modules/strapi/bin/strapi.js",
    "lint": "node_modules/.bin/eslint api/**/*.js config/**/*.js plugins/**/*.js",
    "postinstall": "node node_modules/strapi/lib/utils/post-install.js"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "devDependencies": {
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-scripts": "2.0.4",
    "semantic-ui-css": "^2.4.1",
    "semantic-ui-react": "^0.82.5"
  },
  "dependencies": {
    "if-env": "^1.0.4",
    "lodash": "4.17.11",
    "npm-run-all": "^4.1.3",
    "npm-watch": "^0.4.0",
    "strapi": "3.0.0-alpha.14.3",
    "strapi-hook-mongoose": "3.0.0-alpha.14.3"
  },
  "author": {
    "name": "ston3o"
  },
  "maintainers": [
    {
      "name": "ston3o"
    }
  ],
  "engines": {
    "node": ">= 9.0.0",
    "npm": ">= 5.0.0"
  },
  "license": "GPL-3.0",
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
